{
  "name": "react-postgraphile",
  "version": "0.3.0",
  "private": true,
  "description": "Experiment with Postgraphile and React",
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "apollo-boost": "^0.4.3",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.15",
    "graphql": "^14.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,ts,tsx src",
    "postgraphile": "postgraphile --connection postgres://localhost:5432/react_postgraphile --schema react_postgraphile --token react_postgraphile.jwt_token --default-role react_postgraphile_anonymous --secret your_secret --cors",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "types:gen": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.5.0",
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/typescript": "^1.5.0",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest-fetch-mock": "^2.0.0",
    "postgraphile": "^4.4.2",
    "react-scripts": "^3.0.1",
    "typescript": "^3.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
